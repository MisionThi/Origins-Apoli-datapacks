{
  "type": "origins:multiple",

  "hotbar_changed": {
    "type": "origins:action_over_time",
    "rising_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "function thi_pocket_hand:detect_change/start_setup"
        }
      ]
    },
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "function thi_pocket_hand:detect_change/main"
        }
      ]
    },
    "interval": 1
  }
}
